{% extends 'base.html.twig' %}

{% block title %}Ajout site{% endblock %}

{% block body %}

{{ form_start(form)}}

    {{ form_row(form.nom) }}

    {% if app.request.get('_route') == 'app_admin_sites_ajouter' %}
        <button class="btn btn-success" type="submit">Ajouter</button>
    {% else %}
        <button class="btn btn-success" type="submit">Modifier</button>
    {% endif %}

{{ form_end(form)}}

{% endblock %}
